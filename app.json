{
  "expo": {
    "name": "KISKA",
    "slug": "kiska",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": null,
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "kiska"
  }
}